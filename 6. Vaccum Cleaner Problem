from collections import deque
def vacuum():
    start=(1,1)
    goal=(0,0)
    q=deque([(start,[])])
    v=set([start])
    moves=[(-1,0),(1,0),(0,-1),(0,1)]
    while q:
        (x,y),path=q.popleft()
        if (x,y)==goal:
            for p in path+[(x,y)]:
                print(p)
            print("Reached Goal")
            return
        for dx,dy in moves:
            nx,ny=x+dx,y+dy
            if 0<=nx<=1 and 0<=ny<=1:
                state=(nx,ny)
                if state not in v:
                    v.add(state)
                    q.append((state,path+[(x,y)]))
vacuum()

OUTPUT
(1, 1)
(0, 1)
(0, 0)
Reached Goal
