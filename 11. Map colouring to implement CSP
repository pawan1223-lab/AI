regions = ['WA','NT','SA','Q','NSW','V','T']
colors = ['Red','Green','Blue']
neighbors = {
    'WA':['NT','SA'],
    'NT':['WA','SA','Q'],
    'SA':['WA','NT','Q','NSW','V'],
    'Q':['NT','SA','NSW'],
    'NSW':['Q','SA','V'],
    'V':['SA','NSW'],
    'T':[]
}
def csp(assignment={}):
    if len(assignment)==len(regions):
        print(assignment)
        return True
    unassigned=[r for r in regions if r not in assignment]
    region=unassigned[0]
    for color in colors:
        if all(assignment.get(n)!=color for n in neighbors[region]):
            assignment[region]=color
            if csp(assignment):
                return True
            del assignment[region]
    return False
csp()


OUTPUT
{'WA': 'Red', 'NT': 'Green', 'SA': 'Blue', 'Q': 'Red', 'NSW': 'Green', 'V': 'Red', 'T': 'Red'}
